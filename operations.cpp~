#include <bits/stdc++.h>

using namespace std;

int main()
{
/*
	Variable Declaration
	cOperation:	operation to be done.
	iA && iB:	Numbers to receive the operation;
*/
	char cOperation;
	float iA, iB;
	
	cin >> cOperation;
	cin >> iA >> iB;
	
	switch(cOperation)
	{
		case 'M':
		case 'm':
			printf("%.2f", iA*iB);
			break;
		case 'D':
		case 'd':
			printf("%.2f", iA/iB);
			break;
	}
	printf("\n");
	return 0;
}
